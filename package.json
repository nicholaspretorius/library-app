{
  "name": "library-app",
  "version": "0.0.1",
  "description": "A node.js and Express library application to manage your books",
  "main": "app.js",
  "scripts": {
    "start": "DEBUG=app,app:* nodemon app.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint app.js"
  },
  "keywords": [
    "node.js",
    "express"
  ],
  "author": "Nicholas Pretorius",
  "license": "ISC",
  "dependencies": {
    "body-parser": "1.18.2",
    "chalk": "2.3.2",
    "cookie-parser": "1.4.3",
    "debug": "3.1.0",
    "ejs": "2.5.7",
    "express": "4.16.3",
    "express-session": "1.15.6",
    "mongodb": "3.0.5",
    "morgan": "1.9.0",
    "passport": "0.4.0",
    "passport-local": "1.0.0",
    "pug": "2.0.3"
  },
  "devDependencies": {
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-plugin-import": "2.9.0",
    "nodemon": "1.17.2"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2500",
    "env": {
      "NODE_ENV": "development",
      "PORT": 4001
    }
  }
